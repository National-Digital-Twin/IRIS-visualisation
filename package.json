    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^15.0.2",
    "prettier": "^3.0.3",
    "prettier-eslint": "^16.1.2",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
