{
  "name": "c477-vis",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration=local",
    "start:dev": "ng serve --configuration=dev",
    "start:prod": "ng serve --configuration=production",
    "build": "ng build",
    "build:dev": "ng build --configuration=dev",
    "build:local": "npm run config && ng build --configuration=local",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "prettier": "npx prettier --write .",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "prepare": "husky install",
    "commit": "git-cz",
    "compodoc": "npx compodoc -p tsconfig.doc.json -a screenshots -n \"C477 Vis documentation\" -s -o",
    "config": "ts-node -O '{\"module\": \"commonjs\"}' ./scripts/config.index.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
