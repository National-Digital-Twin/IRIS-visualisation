{
  "name": "iris",
  "version": "1.5.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration=local",
    "start:dev": "ng serve --configuration=dev",
    "start:prod": "ng serve --configuration=production",
    "build": "ng build --output-path dist/iris",
    "build:dev": "ng build --configuration=dev",
    "build:local": "npm run config && ng build --configuration=local",
    "watch": "ng build --watch --configuration development",
    "lint": "ng lint",
    "prettier": "npx prettier --write .",
    "e2e": "cypress open",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "prepare": "husky install",
    "commit": "git-cz",
    "compodoc": "npx compodoc -p tsconfig.doc.json -a screenshots -n \"IRIS documentation\" -s -o",
    "config": "ts-node -O '{\"module\": \"commonjs\"}' ./scripts/config.index.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.2",
    "@angular/cdk": "^17.0.1",
    "@angular/common": "^17.0.2",
    "@angular/compiler": "^17.0.2",
    "@angular/core": "^17.0.2",
    "@angular/forms": "^17.0.2",
    "@angular/material": "^17.0.1",
    "@angular/material-moment-adapter": "^17.0.3",
    "@angular/platform-browser": "^17.0.2",
    "@angular/platform-browser-dynamic": "^17.0.2",
    "@angular/router": "^17.0.2",
    "@arc-web/components": "3.0.0-1",
    "@mapbox/mapbox-gl-draw": "^1.4.3",
    "@turf/bbox": "^6.5.0",
    "@turf/boolean-within": "^6.5.0",
    "@turf/helpers": "^6.5.0",
    "@turf/intersect": "^6.5.0",
    "@turf/points-within-polygon": "^6.5.0",
    "file-saver": "^2.0.5",
    "install": "^0.13.0",
    "jszip": "^3.10.1",
    "mapbox-gl": "3.0.1",
    "npm": "^10.2.4",
    "posthog-js": "^1.100.0",
    "proj4": "^2.10.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.1/xlsx-0.20.1.tgz",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.0",
    "@angular-eslint/builder": "17.0.1",
    "@angular-eslint/eslint-plugin": "17.0.1",
    "@angular-eslint/eslint-plugin-template": "17.0.1",
    "@angular-eslint/schematics": "17.0.1",
    "@angular-eslint/template-parser": "17.0.1",
    "@angular/cli": "^17.0.0",
    "@angular/compiler-cli": "^17.0.2",
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@commitlint/cz-commitlint": "^18.2.0",
    "@compodoc/compodoc": "^1.1.22",
    "@cypress/schematic": "^2.5.1",
    "@types/file-saver": "^2.0.7",
    "@types/geojson": "^7946.0.13",
    "@types/jasmine": "~4.3.0",
    "@types/mapbox__mapbox-gl-draw": "^1.4.6",
    "@types/mapbox-gl": "^2.7.18",
    "@types/papaparse": "^5.3.13",
    "@types/proj4": "^2.5.5",
    "@typescript-eslint/eslint-plugin": "6.11.0",
    "@typescript-eslint/parser": "6.11.0",
    "commitizen": "^4.3.0",
    "cypress": "latest",
